{
  "name": "cordova-gen-icon",
  "version": "0.1.1",
  "description": "Apache Cordova Icon Generator",
  "main": "index.js",
  "bin": {
    "cordova-gen-icon": "bin/cordova-gen-icon"
  },
  "scripts": {
    "jshint": "./node_modules/jshint/bin/jshint index.js bin/* libs/*.js",
    "mocha" : "./node_modules/mocha/bin/mocha -R spec tests/runner.js",
    "test": "node package.json; npm run jshint; npm run mocha"
  },
  "directories": [
    "bin",
    "libs",
    "hooks"
  ],
  "dependencies": {
    "commander": "*",
    "imagemagick": "*",
    "dom-js": "*"
  },
  "devDependencies": {
    "jshint": "*",
    "mocha": "*"
  },
  "repository": {
    "type": "mercurial",
    "url": "ssh://hg@bitbucket.org/ntakimura/cordova-gen-icon"
  },
  "keywords": [
    "cordova",
    "hooks",
    "icon"
  ],
  "author": "Naoki Takimura",
  "license": {
    "type": "WTFPL",
    "url": "http://www.wtfpl.net/about/"
  }
}
